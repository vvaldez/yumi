# Original 64 bit Menu entry created and provided by Andy Campos
# Edits done by Lance for further use with YUMI
default 0
timeout=0

title HP SmartStart 8.60 x32 (use this to deploy OS)
find --set-root /multiboot/HP/ss860_x32/system/initrd.img
kernel /multiboot/HP/ss860_x32/system/vmlinuz media=usb:/multiboot/HP/ss860_x32 rw root=/dev/ram0 ramdisk_size=396332 initrd=/multiboot/HP/ss860_x32/system/initrd.img init=/bin/init loglevel=3 ide=nodma ide=noraid pnpbios=off vga=791 noexec32=off nox2apic splash=silent
initrd /multiboot/HP/ss860_x32/system/initrd.img
boot